{
	"name": "@goobits/store",
	"version": "1.7.0",
	"description": "Reusable e-commerce store components and utilities for Goobits projects built on Medusa v2",
	"main": "index.js",
	"svelte": "./index.js",
	"type": "module",
	"exports": {
		".": {
			"svelte": "./index.js",
			"default": "./index.js"
		},
		"./ui": {
			"svelte": "./ui/index.js",
			"default": "./ui/index.js"
		},
		"./utils": {
			"svelte": "./utils/index.js",
			"default": "./utils/index.js"
		},
		"./stores": {
			"svelte": "./stores/index.js",
			"default": "./stores/index.js"
		},
		"./handlers": {
			"svelte": "./handlers/index.js",
			"default": "./handlers/index.js"
		},
		"./payment": {
			"svelte": "./payment/index.js",
			"default": "./payment/index.js"
		},
		"./config": {
			"svelte": "./config/index.js",
			"default": "./config/index.js"
		},
		"./i18n": {
			"svelte": "./i18n/index.js",
			"default": "./i18n/index.js"
		}
	},
	"keywords": [
		"sveltekit",
		"shop",
		"store",
		"ecommerce",
		"checkout",
		"components"
	],
	"author": "Goobits",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.js",
		"ui",
		"utils",
		"stores",
		"handlers",
		"payment",
		"config",
		"i18n",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"@medusajs/medusa-js": "^6.1.10",
		"qrcode": "^1.5.4"
	},
	"peerDependencies": {
		"@lucide/svelte": "^0.548.0",
		"@stripe/stripe-js": "^3.0.0 || ^4.0.0 || ^5.0.0",
		"@sveltejs/kit": "^2.0.0",
		"formsnap": "^2.0.0",
		"svelte": "^5.0.0",
		"sveltekit-superforms": "^2.25.0",
		"zod": "^3.23.0"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.48.4"
	},
	"scripts": {
		"dev": "echo 'Run from monorepo root'",
		"test": "echo 'No tests configured'"
	},
	"homepage": "https://github.com/goobits/store#readme",
	"bugs": {
		"url": "https://github.com/goobits/store/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/goobits/store.git"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=9.0.0"
	}
}
